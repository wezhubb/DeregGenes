citHeader("To cite DeregGenes in publications use:")

citEntry(entry = "Unpublished",
  title        = "DeregGenes: find deregulated genes",
  author       = "Wenzhu Ye",
  note         = "Unpublished",
  year         = "2022",

  textVersion  =
  paste("Ye, W. (2022) DeregGenes: find deregulated genes.
        Unpublished. URL https://github.com/wezhub/DeregGenes.")
)

